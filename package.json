{
  "name": "template",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/blossomcommunity/template.git",
  "author": "Jack LaFond <jack.lafond@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/ioredis": "^4.22.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "prettier": "^2.2.1",
    "prisma": "^2.20.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.1",
    "fastify-static": "^4.0.1",
    "ioredis": "^4.24.2",
    "node-fetch": "^2.6.1",
    "signale": "^1.4.0",
    "twitter-api-client": "^1.3.7",
    "zod": "^1.11.11"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node .",
    "generate": "prisma generate",
    "lint": "eslint .",
    "services:up": "docker-compose up -f docker-compose.dev.yml -d&&yarn migrate",
    "services:down": "docker-compose -f docker-compose.dev.yml down",
    "services:start": "docker-compose -f docker-compose.dev.yml start",
    "services:stop": "docker-compose -f docker-compose.dev.yml stop",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "up:prod": "docker-compose up --build -d",
    "down:prod": "docker-compose down",
    "prod": "yarn migrate:deploy && yarn start"
  }
}
